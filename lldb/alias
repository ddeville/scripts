# memr $1 --> memory read -f x -s 8 %1
command regex memr 's/(.+)/memory read -f x -s 8 %1/'

# pa $1 --> process attach --name %1
command regex pa 's/(.+)/process attach --name %1/'

# paw $1 --> process attach --waitfor --name %1
command regex paw 's/(.+)/process attach --waitfor --name %1/'

# pf $1 --> register read %1 --format float64
command regex pf 's/(.+)/register read %1 --format float64 %1/'

